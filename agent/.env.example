# Environment Configuration Example

# Application Settings
APP_ENV=development
PROJECT_NAME=Crypto Agent
AGENT_NAME=CryptoAI
VERSION=1.0.0
DEBUG=true
PORT=8010

# CORS Settings
ALLOWED_ORIGINS='["http://localhost:3010", "http://localhost:8010"]'

# MONGODB Settings
MONGO_URI=mongodb://localhost:27017
MONGO_DB_NAME=Crypto_agent

# LLM Settings
GOOGLE_API_KEY=
LLM_MODEL="gemini-2.5-flash"
LLM_TEMPERATURE=0.7

# Qdrant Settings
QDRANT_URL=http://localhost:6010
QDRANT_API_KEY=
QDRANT_COLLECTION_NAME=Crypto_agent
VECTOR_SEARCH_TOP_K=5
VECTOR_SIMILARITY_THRESHOLD=0.5

# Embedding Settings
JINA_EMBEDDING_MODEL=jina-embeddings-v3
JINA_API_KEY=
JINA_EMBEDDING_URL=https://api.jina.ai/v1/embeddings
EMBEDDING_DIMENSION=512

# Chunking Settings
CHUNK_SIZE=1000
CHUNK_OVERLAP=100

# Langfuse Settings
LANGFUSE_PUBLIC_KEY="your-langfuse-public-key"
LANGFUSE_SECRET_KEY="your-langfuse-secret-key"
LANGFUSE_HOST=https://cloud.langfuse.com

# API Settings
API_PREFIX=/api
API_VERSION=v1

# OAuth Settings
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret
GITHUB_REDIRECT_URI=http://localhost:8010/api/auth/callback/github

GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REDIRECT_URI=http://localhost:8010/api/auth/callback/google

# News Scraping parameters
RAG_PROCESSING_INTERVAL_HOURS=2
RAG_RUN_ON_STARTUP=true